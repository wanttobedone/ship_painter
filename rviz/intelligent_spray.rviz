Panels:
  - Class: rviz/Displays
    Help Height: 78
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Global Options1
        - /Status1
        - /智能喷涂路径1
        - /无人机状态1
        - /视觉反馈1
        - /系统状态1
      Splitter Ratio: 0.5
    Tree Height: 300
  - Class: rviz/Selection
    Name: Selection
  - Class: rviz/Tool Properties
    Expanded:
      - /2D Pose Estimate1
      - /Publish Point1
    Name: Tool Properties
    Splitter Ratio: 0.5886790156364441
  - Class: rviz/Views
    Expanded:
      - /Current View1
    Name: Views
    Splitter Ratio: 0.5
  - Class: rviz/Time
    Name: Time
    SyncMode: 0
    SyncSource: ""
Preferences:
  PromptSaveOnExit: true
Toolbars:
  toolButtonStyle: 2
Visualization Manager:
  Class: ""
  Displays:
    # 基础网格
    - Alpha: 0.5
      Cell Size: 1
      Class: rviz/Grid
      Color: 160; 160; 164
      Enabled: true
      Line Style:
        Line Width: 0.029999999329447746
        Value: Lines
      Name: Grid
      Normal Cell Count: 0
      Offset:
        X: 0
        Y: 0
        Z: 0
      Plane: XY
      Plane Cell Count: 30
      Reference Frame: map
      Value: true
      
    # 智能喷涂路径可视化
    - Class: rviz/MarkerArray
      Enabled: true
      Marker Topic: /visualization/spray_path
      Name: 智能喷涂路径
      Namespaces:
        spray_path: true
        visual_updates: true
      Queue Size: 100
      Value: true
      
    # 无人机当前位置
    - Alpha: 1
      Axes Length: 0.5
      Axes Radius: 0.05
      Class: rviz/Pose
      Color: 255; 25; 0
      Enabled: true
      Head Length: 0.15
      Head Radius: 0.05
      Name: 无人机位置
      Queue Size: 10
      Shaft Length: 0.4
      Shaft Radius: 0.02
      Shape: Arrow
      Topic: /iris_0/mavros/local_position/pose
      Unreliable: false
      Value: true
      
    # 无人机飞行轨迹
    - Alpha: 1
      Buffer Length: 1
      Class: rviz/Path
      Color: 25; 255; 0
      Enabled: true
      Head Diameter: 0.30000001192092896
      Head Length: 0.20000000298023224
      Length: 0.30000001192092896
      Line Style: Lines
      Line Width: 0.029999999329447746
      Name: 飞行轨迹
      Offset:
        X: 0
        Y: 0
        Z: 0
      Pose Color: 255; 85; 255
      Pose Style: None
      Queue Size: 10
      Radius: 0.029999999329447746
      Shaft Diameter: 0.10000000149011612
      Shaft Length: 0.10000000149011612
      Topic: /iris_0/mavros/local_position/pose
      Unreliable: false
      Value: true
      
    # 当前目标航点
    - Alpha: 1
      Axes Length: 0.3
      Axes Radius: 0.03
      Class: rviz/Pose
      Color: 255; 255; 0
      Enabled: true
      Head Length: 0.1
      Head Radius: 0.03
      Name: 当前目标
      Queue Size: 10
      Shaft Length: 0.25
      Shaft Radius: 0.015
      Shape: Arrow
      Topic: /spray_system/current_target
      Unreliable: false
      Value: true
      
    # 系统状态显示
    - Class: rviz/MarkerArray
      Enabled: true
      Marker Topic: /visualization/system_status
      Name: 系统状态
      Namespaces:
        system_status: true
      Queue Size: 100
      Value: true
      
    # 坐标系变换
    - Class: rviz/TF
      Enabled: true
      Filter (blacklist): ""
      Filter (whitelist): ""
      Frame Timeout: 15
      Frames:
        All Enabled: true
        base_link:
          Value: true
        base_link_frd:
          Value: true
        camera_link:
          Value: true
        map:
          Value: true
        map_ned:
          Value: true
        odom:
          Value: true
        odom_ned:
          Value: true
        world:
          Value: true
        aruco_marker_frame:
          Value: true
      Marker Alpha: 1
      Marker Scale: 0.5
      Name: TF
      Show Arrows: true
      Show Axes: true
      Show Names: true
      Tree:
        world:
          map:
            map_ned:
              {}
            base_link:
              base_link_frd:
                camera_link:
                  {}
        odom:
          odom_ned:
            {}
      Update Interval: 0
      Value: true
      
    # 相机图像 (可选)
    - Class: rviz/Image
      Enabled: false
      Image Topic: /iris_0/camera/image_raw
      Max Value: 1
      Median window: 5
      Min Value: 0
      Name: 相机图像
      Normalize Range: true
      Queue Size: 2
      Transport Hint: raw
      Unreliable: false
      Value: false
      
    # 深度图像 (可选)
    - Class: rviz/Image
      Enabled: false
      Image Topic: /iris_0/camera/depth/image_raw
      Max Value: 10
      Median window: 5
      Min Value: 0
      Name: 深度图像
      Normalize Range: true
      Queue Size: 2
      Transport Hint: raw
      Unreliable: false
      Value: false
      
    # 点云数据 (可选)
    - Alpha: 1
      Autocompute Intensity Bounds: true
      Autocompute Value Bounds:
        Max Value: 10
        Min Value: -10
        Value: true
      Axis: Z
      Channel Name: intensity
      Class: rviz/PointCloud2
      Color: 255; 255; 255
      Color Transformer: Intensity
      Decay Time: 0
      Enabled: false
      Invert Rainbow: false
      Max Color: 255; 255; 255
      Min Color: 0; 0; 0
      Name: 激光点云
      Position Transformer: XYZ
      Queue Size: 10
      Selectable: true
      Size (Pixels): 3
      Size (m): 0.01
      Style: Flat Squares
      Topic: /iris_0/velodyne_points
      Unreliable: false
      Use Fixed Frame: true
      Use rainbow: true
      Value: false
      
    # ArUco标记检测结果
    - Class: rviz/MarkerArray
      Enabled: true
      Marker Topic: /aruco_single/result
      Name: ArUco标记
      Namespaces:
        {}
      Queue Size: 100
      Value: true
      
    # 喷涂区域可视化
    - Class: rviz/Marker
      Enabled: true
      Marker Topic: /visualization/spray_coverage
      Name: 喷涂覆盖区域
      Namespaces:
        spray_coverage: true
      Queue Size: 100
      Value: true
      
    # 安全区域显示
    - Class: rviz/Marker
      Enabled: true
      Marker Topic: /visualization/safety_zone
      Name: 安全区域
      Namespaces:
        safety_zone: true
      Queue Size: 100
      Value: true
      
  Enabled: true
  Global Options:
    Background Color: 48; 48; 48
    Default Light: true
    Fixed Frame: map
    Frame Rate: 30
  Name: root
  Tools:
    - Class: rviz/Interact
      Hide Inactive Objects: true
    - Class: rviz/MoveCamera
    - Class: rviz/Select
    - Class: rviz/FocusCamera
    - Class: rviz/Measure
    - Class: rviz/SetInitialPose
      Theta std deviation: 0.2617993950843811
      Topic: /initialpose
      X std deviation: 0.5
      Y std deviation: 0.5
    - Class: rviz/SetGoal
      Topic: /move_base_simple/goal
    - Class: rviz/PublishPoint
      Single click: true
      Topic: /clicked_point
  Value: true
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 15.0
      Enable Stereo Rendering:
        Stereo Eye Separation: 0.05999999865889549
        Stereo Focal Distance: 1
        Swap Stereo Eyes: false
        Value: false
      Field of View: 0.7853981852531433
      Focal Point:
        X: 5.0
        Y: 0.0
        Z: 1.5
      Focal Shape Fixed Size: true
      Focal Shape Size: 0.05000000074505806
      Invert Z Axis: false
      Name: Current View
      Near Clip Distance: 0.009999999776482582
      Pitch: 0.7
      Target Frame: map
      Yaw: 0.8
    Saved: ~
Window Geometry:
  Displays:
    collapsed: false
  Height: 800
  Hide Left Dock: false
  Hide Right Dock: false
  QMainWindow State: 
  Selection:
    collapsed: false
  Time:
    collapsed: false
  Tool Properties:
    collapsed: false
  Views:
    collapsed: false
  Width: 1267
  X: 67
  Y: 60